import java.util.Comparator;
	 
	public class NameComparator implements Comparator<Product>
	{
	 
	    @Override
	    public int compare(Product p1, Product p2) {
	        if(p2 == null) return -1;
	        if(p1.getName() > p2.(int)getName()) return 1;
	        else if(p1.getName() < p2.getName()) return -1;
	        else return 0;
	    }
	}
